          <button id="submit" class="btn btn-primary float-right">Submit</button>
        </form>
        <div id = container>
        <h3>Goals:</h3>

        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</span></a>
            <button class="btn btn-danger float-right delete"></i>Delete</button>
          </li>
          {{/each}}
        </ul>
        </div>
