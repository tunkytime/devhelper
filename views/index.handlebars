<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h2>My Goals!</h2>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="example-text">New Goal</label>
            <input type="text" id="example-text" class="form-control" aria-describedby="Enter New Goal Here" placeholder="Enter Example Text">
          </div>
          {{!-- <div class="form-group">
            <label for="example-description">Goal Description</label>
            <textarea class="form-control" id="example-description" aria-describedby="example-description" placeholder="Enter Goal Description"></textarea>
